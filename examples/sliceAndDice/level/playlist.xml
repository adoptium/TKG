<playlist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../resources/playlist.xsd">
	<test>
		<testCaseName>testDefault</testCaseName>
		<command>echo "default is extended"; \
	$(TEST_STATUS)</command>
	</test>

	<test>
		<testCaseName>testSanity</testCaseName>
		<command>echo "sanity"; \
	$(TEST_STATUS)</command>
		<levels>
			<level>sanity</level>
		</levels>
	</test>

	<test>
		<testCaseName>testExtended</testCaseName>
		<command>echo "extended"; \
	$(TEST_STATUS)</command>
		<levels>
			<level>extended</level>
		</levels>
	</test>

	<test>
		<testCaseName>testDev</testCaseName>
		<command>echo "dev"; \
	$(TEST_STATUS)</command>
		<levels>
			<level>dev</level>
		</levels>
	</test>

	<test>
		<testCaseName>testSpecial</testCaseName>
		<command>echo "special"; \
	$(TEST_STATUS)</command>
		<levels>
			<level>special</level>
		</levels>
	</test>
</playlist>
